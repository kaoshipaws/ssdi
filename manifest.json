{
	"manifest_version": 3,
	"name": "SSDI Steam Selecting Duplicate Items",
	"version": "1.0",
	"description": "Selecting duplicate items in the steam inventory",
	"permissions": [
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"https://steamcommunity.com/id/*/inventory*"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "ic-16.png",
			"48": "ic-48.png",
			"128": "ic-128.png"
		}
	}
}